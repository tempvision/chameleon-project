{
    "extends": ["tslint-angular"],
    "rules": {
      "directive-selector": [true, "attribute", "foo", "camelCase"],
      "component-selector": [true, "element", "foo", "kebab-case"],
      "only-arrow-functions": true,
      "prefer-const": true,
      "no-unnecessary-initializer": true,
      "no-duplicate-imports": true

    }
  }